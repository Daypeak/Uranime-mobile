<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:top="0dp" android:left="-2dp" android:right="-2dp"> 
	      <shape 
	        android:shape="rectangle">
	            <stroke android:width="2dp" android:color="#FF1988d0" />
	            <solid android:color="#00000000" />
	        </shape>
	</item>
	<item>
		<shape
		    xmlns:android="http://schemas.android.com/apk/res/android"
		    android:shape="rectangle">
		    <gradient
		        android:angle="90"
		        android:centerColor="#014b8b"
		        android:endColor="#005f9f"
		        android:startColor="#023979"
		        android:type="linear" />
		</shape>
	</item>
	<item android:bottom="0dp" android:left="-2dp" android:right="-2dp"> 
	      <shape 
	        android:shape="rectangle">
	            <stroke android:width="2dp" android:color="#FF104b87" />
	            <solid android:color="#00000000" />
	        </shape>
	   </item>
</layer-list>